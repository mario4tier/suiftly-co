{
  "env": {
    "SEAL_REFERENCE_PATH": "~/repos/seal-reference-main",
    "BASH_DEFAULT_TIMEOUT_MS": "600000",
    "BASH_MAX_TIMEOUT_MS": "900000"
  },
  "statusLine": {
    "type": "command",
    "command": "if [ -f .claude-repo-status ]; then head -n 1 .claude-repo-status; else echo '‚è≥ Repos pending'; fi"
  },
  "permissions": {
    "additionalDirectories": ["~/repos"],
    "allow": [
      "Bash(/dev/null)",
      "Bash(:*cargo:*)",
      "Bash(bash:*)",
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(cp:*)",
      "Bash(curl:*)",
      "Bash(diff:*)",
      "Bash(do)",
      "Bash(done)",
      "Bash(echo:*)",
      "Bash(env:*)",
      "Bash(export:*)",
      "Bash(fi)",
      "Bash(find:*)",
      "Bash(for:*)",
      "Bash(gh api:*)",
      "Bash(gh issue list:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)",
      "Bash(git:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(jq:*)",
      "Bash(ls:*)",
      "Bash(make:*)",
      "Bash(mv:*)",
      "Bash(node:*)",
      "Bash(npm run build:*)",
      "Bash(npx:*)",
      "Bash(uvx:*)",
      "Bash(pnpm:*)",
      "Bash(prettier:*)",
      "Bash(rm:*)",
      "Bash(sed:*)",
      "Bash(sort:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Bash(then)",
      "Bash(timeout:*)",
      "Bash(tsc:*)",
      "Bash(uniq:*)",
      "Bash(wc:*)",
      "Bash(wget:*)",
      "Bash(yarn:*)",
      "Bash(~/walrus/:*)",
      "Bash(~/walrus/scripts/:*)",
      "Bash(~/walrus/scripts/dev/:*)",
      "Read(~/repos/**)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:docs.sui.io)",
      "WebFetch(domain:stackoverflow.com)",
      "WebSearch"
    ],
    "deny": [],
    "ask": []
  }
}
